{
	"service" : {
		"name" : "prediction",
		"host" : "{{ ansible_user_ip }}",
		"port" : 8802
	},
	"api-gateway" : {
		"port": 8787,
		"host": "{{ ansible_user_ip }}"
	},
	"metrics" : {
		"enabled": true,
		"port": 8916
	},
	"ssl": {
		"use": false
	},
	"access": {
		"use": false
	},
	"ta-models-file-path": "./conf/demoThreatActorModels.json",
    "product-classifiers-file-path": "./conf/cpeClassifiers.json",

	"database" : {
		"collection": "preproduction",
		"class": "de.acs.prediction.database.mongo.PredictionDatabaseMongoDB",
		"serverAddresses": [
		  {
			"host": "{{ ansible_user_ip }}",
			"port": 27017
		  }
	   ], 
	   "readPreference" : "secondary",
	   "name": "threat-prediction",
	   "timeout": 5000

	}
}
